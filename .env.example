# ==============================
# Dashboard Oficina Doctor Auto
# Variáveis de Ambiente
# ==============================

# --- Server ---
NODE_ENV=development
PORT=3000
DATABASE_URL=mysql://user:password@host:3306/database
JWT_SECRET=your_jwt_secret_here

# --- OAuth (Manus platform) ---
VITE_APP_ID=your_app_id
OAUTH_SERVER_URL=https://your-oauth-server
VITE_OAUTH_PORTAL_URL=https://your-oauth-portal
OWNER_OPEN_ID=your_owner_open_id

# --- Trello ---
TRELLO_API_KEY=your_trello_api_key
TRELLO_TOKEN=your_trello_token
TRELLO_BOARD_ID=your_trello_board_id
TRELLO_LIST_ID_AGENDADOS=your_list_id
TRELLO_WEBHOOK_SECRET=your_webhook_secret

# Também acessível no client via Vite (prefixo VITE_)
VITE_TRELLO_API_KEY=your_trello_api_key
VITE_TRELLO_TOKEN=your_trello_token
VITE_TRELLO_BOARD_ID=your_trello_board_id

# --- Telegram ---
TELEGRAM_BOT_TOKEN=your_telegram_bot_token
TELEGRAM_CHAT_ID=your_telegram_chat_id

# --- Supabase ---
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# --- Kommo (CRM) ---
KOMMO_ACCESS_TOKEN=your_kommo_access_token
KOMMO_ACCOUNT_DOMAIN=https://your-account.kommo.com

# --- Forge API (opcional) ---
BUILT_IN_FORGE_API_URL=
BUILT_IN_FORGE_API_KEY=
